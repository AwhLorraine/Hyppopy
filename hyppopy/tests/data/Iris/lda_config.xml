<hyppopy>
	<hyperparameter>
		<solver>
			<domain>categorical</domain>
			<data>[svd,lsqr,eigen]</data>
			<type>str</type>
		</solver>
		<shrinkage>
			<domain>uniform</domain>
			<data>[0.0,1.0]</data>
			<type>float</type>
		</shrinkage>
	</hyperparameter>
	<settings>
		<solver_plugin>
			<max_iterations>3</max_iterations>
			<use_plugin>hyperopt</use_plugin>
			<output_dir>D:/Projects/Python/hyppopy/hyppopy/tests/data/Iris</output_dir>
		</solver_plugin>
		<custom>
			<data_path>D:/Projects/Python/hyppopy/hyppopy/tests/data/Iris</data_path>
			<data_name>train_cleaned.csv</data_name>
			<labels_name>Survived</labels_name>
		</custom>
	</settings>
</hyppopy>